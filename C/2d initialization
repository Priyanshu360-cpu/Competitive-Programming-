#include <stdio.h>
#define newline printf("\n");
int main(void) {
	int a[1][2];
	for(int i = 0;i<=1;i++){
	    scanf("%d",&a[i]);
	for(int j = 0;j<=2;j++){
	    scanf("%d",&a[i][j]);
	}
	}
	for(int i = 0;i<=1;i++){
	for(int j = 0;j<=2;j++){
	    printf("%d",a[i][j]);
	    newline
	} 
	newline
	}
	return 0;
}

